#: account.cpp:860
msgctxt "@info Warning: the server does'nt support secure login"
msgid ""
"Account <resource>%1</resource>: This server doesn't provide a safety login "
"and you have disallowed the using of an unsafe login. If you want to use "
"this Account you must allow unsafe login at the account setup.<nl/"
"><warning>Bear in mind in this case criminals could read your password!</"
"warning>"
msgstr ""
"Account <resource>%1</resource>: Deze server biedt geen beveiligde login en "
"u heeft niet aangegeven onbeveligde login toe te staan. Als u deze Account wilt "
"gebruiken, moet u onbeveiligde login toestaan in de accountinstellingen.<nl/> "
"<warning>Houd in gedachte dat op deze manier criminelen uw wachtwoord kunnen lezen!</"
"warning>"

#: account.cpp:860 account.cpp:2427
msgctxt "@title:window"
msgid "Unsafe login is not allowed"
msgstr "Onbeveiligde login is niet toegestaan"

#: account.cpp:936
msgctxt "@info error message: we could not quit the POP3 session"
msgid ""
"<resource>%1</resource> has not accepted the <icode>%2</icode> command. "
"Error message is: <message>%3</message>"
msgstr "<resource>%1</resource> heeft het <icode>%2</icode> commando niet geaccepteerd."

#: account.cpp:969 account.cpp:1003
msgctxt "@info error message"
msgid "Login has failed. Error message is: <message>%1</message>"
msgstr "Inloggen is mislukt. Foutmelding is: <message>%1</message>"

#: account.cpp:1077
msgctxt "@info error message"
msgid ""
"Login has failed. Error message is: <message>%1</message><nl/>Maybe the "
"secure login of this server is faulty. You can try to allow the unsafe login "
"for this account at the account setup.<nl/><warning>Bear in mind in this "
"case criminals could read your password!</warning>"
msgstr ""
"Inloggen is mislukt. Foutmelding is: <message>%1</message><nl/>Misschien is "
"er een fout in de beveiligde login van deze server. U kunt proberen onbeveiligd inloggen "
"in te schakelen voor deze account in de accountinstellingen.<nl/><warning>Houd in gedachte "
"dat op deze manier criminelen uw wachtwoord kunnen lezen!</warning>"

#: account.cpp:1150
msgctxt "@info error message: a server doesn't support mail UIDs"
msgid ""
"<resource>%1</resource> doesn't support mail UID's. KShowmail can't work "
"without this. Error message is: <message>%2</message>"
msgstr ""
"<resource>%1</resource> ondersteunt geen mail UID's. KShowmail kan niet "
"werken zonder dit. Foutmelding is: <message>%2</message>"

#: account.cpp:1187
msgctxt "@info error message"
msgid "Get corrupt UID list. No spaces"
msgstr "Verkrijg corrupte UID-lijst. Geen spaties"

#: account.cpp:1200
msgctxt "@info error message"
msgid "Get corrupt UID list. No number found at begin."
msgstr "Verkrijg corrupte UID-lijst. Geen nummer gevonden aan het begin."

#: account.cpp:1382
msgctxt "@info error message: a server has not send a response"
msgid ""
"<resource>%1</resource> has not sent a response after <icode>%2</icode> "
"command."
msgstr ""
"<resource>%1</resource> heeft geen antwoord gegeven na <icode>%2</icode> "
"commando."

#: account.cpp:1389
msgctxt "@info error message: a server has not send the mail sizes"
msgid "Error while try to get the mail sizes. Message is: <message>%1</message>"
msgstr "Fout bij het verkrijgen van de mailgroottes. Melding is: <message>%1</message>"

#: account.cpp:1406
msgctxt "@info error message"
msgid "Error while try to get the mail sizes. All mails are disappeard."
msgstr "Fout bij het verkrijgen van de mailgroottes. Alle e-mails zijn verdwenen."

#: account.cpp:1422
msgctxt "@info error message"
msgid "Get corrupt size list. No spaces"
msgstr "Verkrijg corrupte UID-lijst. Geen spaties"

#: account.cpp:1434
msgctxt "@info error message"
msgid "Get corrupt size list. No numbers at begin."
msgstr "Verkrijg corrupte UID-lijst.  Geen nummers aan het begin."

#: account.cpp:1446
msgctxt "@info error message"
msgid "Get corrupt size list. No sizes found at end."
msgstr "Verkrijg corrupte UID-lijst. Geen groottes gevonden aan het einde."

#: account.cpp:1510
msgctxt "@info error message: could not get a mail header"
msgid "<resource>%1</resource> has not sent the header of mail <numid>%2</numid>."
msgstr "<resource>%1</resource> heeft geen berichtkop gestuurd <numid>%2</numid>."

