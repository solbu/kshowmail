msgctxt "@info error message: we could not download a mail"
msgid ""
"Error while downloading mail <numid>%1</numid> of <resource>%2</resource>: "
"<message>%3</message>"
msgstr "Fout tijdens het downloaden van mail <numid>%1</numid> van <resource>%2</resource>: "

#: account.cpp:1854
msgctxt "@info error message: we could not download a mail"
msgid ""
"<resource>%1</resource> has not sent a answer after retrieve of mail <numid>%"
"2</numid>."
msgstr ""
"<resource>%1</resource> heeft geen antwoord gegeven na het verkrijgen van mail "
"<numid>%2</numid>"

