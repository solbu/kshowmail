msgstr "Ingen støtte for START-TLS"

#: account.cpp:868
msgctxt "@info Warning: the server does'nt support secure login"
msgid ""
"Account <resource>%1</resource>: This server doesn't provide a safety login "
"and you have disallowed the using of an unsafe login. If you want to use "
"this Account you must allow unsafe login at the account setup.<nl/"
"><warning>Bear in mind, in this case criminals could read your password!</"
"warning>"
msgstr ""
"Konto <resource>%2</resource>: Denne tjeneren støtter ikke sikker påmelding"
"og du har ikke tillatt usikker påmelding. Hvis du vil bruke denne kontoen"
"må du tillate usikker påmelding i konto-konfigurasjonen.<nl/"
"><warning>Husk at i dette tilfellet kan kriminelle være i stand til å lese"
"passordet ditt!</"
"warning>"

#: account.cpp:868 account.cpp:2456
msgctxt "@title:window"
msgid "Unsafe login is not allowed"
msgstr "Usikker påmelding er ikke tillatt"

#: account.cpp:944
msgctxt "@info error message: we could not quit the POP3 session"
msgid ""
"<resource>%1</resource> has not accepted the <icode>%2</icode> command. "
"Error message is: <message>%3</message>"
msgstr ""
"<resource>%1</resource> har ikke akseptert kommandoen <icode>%2</icode>"
"Feilmeldingen er: <message>%3</message>"

#: account.cpp:977
msgctxt "@info error message"
msgid ""
"Login has failed. Maybe your user name is invalid. Error message is: "
"<message>%1</message>"
msgstr ""
"Innlogging slo feil. Kanskje brukernavnet ditt er feil. Feilmeldingen er: <"
"message>%1</message>"

#: account.cpp:1014
msgctxt "@info error message"
msgid ""
"Login has failed. Maybe your user name or password is invalid. Error message "
"is: <message>%1</message>"
msgstr ""
"Innlogging slo feil. Kanskje brukernavnet eller passordet ditt er feil. "
"Feilmeldingen er: <message>%1</message>"

#: account.cpp:1090
msgctxt "@info error message"
msgid ""
"Login has failed. Maybe your user name or password is invalid. Error message "
"is: <message>%1</message><nl/>Maybe the secure login of this server is "
"faulty. You can try to allow the unsafe login for this account at the "
"account setup.<nl/><warning>Bear in mind, in this case criminals could read "
"your password!</warning>"
msgstr ""
"Innlogging slo feil. Kanskje brukernavnet eller passordet ditt er feil. "
"Feilmeldingen er: <message>%1</message><nl/>Sikker innlogging er kanskje "
"feilaktig på denne serveren. Du kan tillate usikker innlogging via "
"konfigurasjonen.<nl/><warning>Husk at i det tilfellet kan kriminelle være i stand "
"til å lese passordet ditt!</warning>"

#: account.cpp:1165
msgctxt "@info error message: a server doesn't support mail UIDs"
msgid ""
"<resource>%1</resource> doesn't support mail UID's. KShowmail can't work "
"without this. Error message is: <message>%2</message>"
msgstr ""
"<resource>%1</resource> støtter ikke mail UID (=Unique Identifier). KShowmail "
"fungerer ikke uten dette. Feilmeldingen er: <message>%2</message>"

#: account.cpp:1202
msgctxt "@info error message"
msgid "Get corrupt UID list. No spaces"
msgstr "Hent korrupt UID liste. Ingen mellomrom"

#: account.cpp:1215
msgctxt "@info error message"
msgid "Get corrupt UID list. No number found at begin."
msgstr "Hent korrupt UID liste. Fant ingen nummer i begynnelsen."

#: account.cpp:1397
msgctxt "@info error message: a server has not send a response"
msgid ""
"<resource>%1</resource> has not sent a response after <icode>%2</icode> "
"command."
msgstr ""
"<resource>%1</resource> har ikke sendt et svar etter <icode>%2</icode> kommandoen."

#: account.cpp:1404
msgctxt "@info error message: a server has not send the mail sizes"
msgid "Error while trying to get the mail sizes. Message is: <message>%1</message>"
msgstr ""
"Feil ved forsøk på å lese størrelsene på e-post. Meldingen er: <message>%1<"
"/message>"

#: account.cpp:1421
msgctxt "@info error message"
msgid "Error while trying to get the mail sizes. All mails are disappeard."
msgstr "Feil ved forsøk på å lese størrelsene på e-post. All e-post har forsvunnet."

#: account.cpp:1437
msgctxt "@info error message"
msgid "Get corrupt size list. No spaces"
msgstr "Hent korrupt størrelse-liste. Ingen mellomrom"

#: account.cpp:1449
msgctxt "@info error message"
msgid "Get corrupt size list. No numbers at begin."
msgstr "Hent korrupt størrelse-liste. Ingen nummer i begynnelsen."

#: account.cpp:1461
msgctxt "@info error message"
msgid "Get corrupt size list. No sizes found at end."
msgstr "Hent korrupt størrelse-liste. Fant ingen nummer på slutten."

#: account.cpp:1525
msgctxt "@info error message: could not get a mail header"
msgid "<resource>%1</resource> has not sent the header of mail <numid>%2</numid>."
msgstr ""
"<resource>%1</resource> har ikke sendt e-post overskrift (header) <numid>%2<"
"/numid>."

#: account.cpp:1533
msgctxt "@info error message: a server doesn't know the command TOP"
msgid ""
"<resource>%1</resource> doesn't support the TOP command. KShowmail can't "
"work without this. Error message is: <message>%2</message>"
msgstr ""
"<resource>%1</resource> støtter ikke TOP-kommandoen. KShowmail fungerer ikke uten "
"dette. Feilmeldingen er: <message>%2</message>"

#: account.cpp:1684
msgctxt "@info error message: no server responce after we have deleted a mail"
msgid ""
"<resource>%1</resource> has not sent an answer after removing of mail <numid>%"
"2</numid>."
msgstr ""
"<resource>%1</resource> har ikke sendt et svar etter sletting av e-post <numid>%"
"2</numid>."

#: account.cpp:1692
msgctxt "@info error message: error while removing a mail"
msgid ""
"Error while removing mail <numid>%1</numid> of <resource>%2</resource>: "
"<message>%3</message>"
msgstr ""
"Feil ved sletting av e-post <numid>%1</numid> av <resource>%2</resource>: "
"<message>%3</message>"

#: account.cpp:1738 account.cpp:1881
msgctxt "@info error message: we could not download a mail"
msgid ""
"<resource>%1</resource> has not sent an answer after retrieve of mail <numid>"
"%2</numid>."
msgstr ""
"<resource>1</resource> har ikke sendt et svar etter henting av e-post <numid>"
"%2</numid>."

#: account.cpp:1746 account.cpp:1889
msgctxt "@info error message: we could not download a mail"
msgid ""
"Error while downloading mail <numid>%1</numid> of <resource>%2</resource>: "
"<message>%3</message>"
msgstr ""
"Feil ved nedlasting av e-post <numid>%1</numid> av <resource>%2</resource>: "
"<message>%3</message>"

#: account.cpp:2106
msgctxt "@info error message: writing a mail into a mailbox: this is not a mail box"
msgid "<filename>%1</filename> is not a mailbox."
msgstr "<filename>%1</filenam> er ikke en elektronisk postboks."

#: account.cpp:2121
msgctxt "@info error message"
msgid ""
"Can't read the hostname of your computer. But KShowmail need it to write a "
"mail into the mailbox."
msgstr ""
"Kan ikke lese din maskins hostnavn. Men KShowmail trenger det for å skrive en "
"e-post i e-postboksen."

#: account.cpp:2145
msgctxt "@info error message: we could not write a mail into a mailbox"
msgid "Could not file a mail to <filename>%1</filename>."
msgstr "Kunne ikke skrive en e-post til <filename>%1</filename>."

#: account.cpp:2156
msgctxt "@info error message: error during writing a mail into a mailbox"
msgid ""
"Could not move a mail from <filename>%1</filename> to <filename>%2</"
"filename>."
msgstr ""
"Kunne ikke flytte en e-post fra <filename>%1</filename> til <filename>%2<"
"/filename>."

#: account.cpp:2197
msgctxt "@info"
msgid ""
"You want to check your mails for spam, but SpamAssassin is not running.<nl/"
">KShowmail skips the spam check."
msgstr ""
"Du vil sjekke din e-post for spam, men SpamAssassin kjører ikke.<nl/>"
"KShowmail dropper spam-sjekken."

#: account.cpp:2197
msgctxt "@title:window"
msgid "SpamAssassin is not running"
msgstr "SpamAssassin kjører ikke."

#: account.cpp:2302
msgctxt "@info error message: general SSL error"
msgid "SSL error: <message>%1</message><nl/>Do you want to continue?"
msgstr "SSL feil: <message>%1</message><nl/>Vil du fortsette?"

#: account.cpp:2303
msgctxt "@title:window"
msgid "SSL-Error - %1"
msgstr "SSL-feil - %1"

#: account.cpp:2398
msgctxt "@info error message"
msgid "Timeout"
msgstr "Tidsoverskridelse"

#: account.cpp:2406
msgctxt "@info the task was canceled by user"
msgid "Task canceled"
msgstr "Oppgave annullert"

#: account.cpp:2456
msgctxt "@info error message: this server doesn't support secure login"
msgid ""
"Account <resource>%1</resource>: This server doesn't provide a safety login "
"and you have disallowed the using of an unsafe login. If you want to use "
"this Account you must allow unsafe login at the account setup.<nl/"
"><warning>Bear in mind in this case criminals could read your password!</"
"warning>"
msgstr ""
"Konto <resource>%1</resource>: Denne tjeneren støtter ikke sikker påmelding"
"og du har forbudt bruk av usikker påmelding. Hvis du vil bruke denne kontoen"
"må du tillate usikker påmelding i konto-konfigurasjonen.<nl/"
"><warning>Husk at i dette tilfellet kan kriminelle være i stand til å lese"
"passordet ditt!</"
"warning>"

