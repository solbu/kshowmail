#: account.cpp:860
msgctxt "@info Warning: the server does'nt support secure login"
msgid ""
"Account <resource>%1</resource>: This server doesn't provide a safety login "
"and you have disallowed the using of an unsafe login. If you want to use "
"this Account you must allow unsafe login at the account setup.<nl/"
"><warning>Bear in mind in this case criminals could read your password!</"
"warning>"
msgstr ""
"Cuenta <resource>%1</resource>: Este servidor no proporciona un inicio de "
"sesión seguro y ha desactivado el uso de inicio de sesión sin seguridad. Si "
"quiere utilizar esta Cuenta debe permitir el inicio de sesión sin seguridad "
"en la configuración de la cuenta.<nl/><warning>¡Tenga en cuenta que en este "
"caso los criminales podrían leer su contraseña!</warning>"

#: account.cpp:860 account.cpp:2427
msgctxt "@title:window"
msgid "Unsafe login is not allowed"
msgstr "No se admite el inicio de sesión sin seguridad"

#: account.cpp:936
msgctxt "@info error message: we could not quit the POP3 session"
msgid ""
"<resource>%1</resource> has not accepted the <icode>%2</icode> command. "
"Error message is: <message>%3</message>"
msgstr ""
"<resource>%1</resource> no ha aceptado el comando <icode>%2</icode>. Mensaje "
"de error: <message>%3</message>"

#: account.cpp:969 account.cpp:1003
msgctxt "@info error message"
msgid "Login has failed. Error message is: <message>%1</message>"
msgstr ""
"Ha fallado el inicio de sesión. Mensaje de error: <message>%1</message>"

#: account.cpp:1077
msgctxt "@info error message"
msgid ""
"Login has failed. Error message is: <message>%1</message><nl/>Maybe the "
"secure login of this server is faulty. You can try to allow the unsafe login "
"for this account at the account setup.<nl/><warning>Bear in mind in this "
"case criminals could read your password!</warning>"
msgstr ""
"Ha fallado el inicio de sesión. El mensaje de error es: <message>%1</"
"message><nl/>Es posible que el inicio de sesión seguro de este servidor "
"tenga algún problema. Puede intentar permitir el inicio de sesión sin "
"seguridad en la configuración de la cuenta.<nl/><warning>¡Tenga en cuenta "
"que en este caso los criminales podrían leer su contraseña!</warning>"

#: account.cpp:1150
msgctxt "@info error message: a server doesn't support mail UIDs"
msgid ""
"<resource>%1</resource> doesn't support mail UID's. KShowmail can't work "
"without this. Error message is: <message>%2</message>"
msgstr ""
"<resource>%1</resource> no admite UID de correo. KShowmail no puede "
"funcionar sin ésto. Mensaje de error: <message>%2</message>"

#: account.cpp:1187
msgctxt "@info error message"
msgid "Get corrupt UID list. No spaces"
msgstr "Lista de UID corrupta. Sin espacios"

#: account.cpp:1200
msgctxt "@info error message"
msgid "Get corrupt UID list. No number found at begin."
msgstr "Lista de UID corrupta. Sin números al inicio."

#: account.cpp:1382
msgctxt "@info error message: a server has not send a response"
msgid ""
"<resource>%1</resource> has not sent a response after <icode>%2</icode> "
"command."
msgstr ""
"<resource>%1</resource> no ha enviado una respuesta tras el comando <icode>%"
"2</icode>."

#: account.cpp:1389
msgctxt "@info error message: a server has not send the mail sizes"
msgid ""
"Error while try to get the mail sizes. Message is: <message>%1</message>"
msgstr ""
"Error al intentar obtener el tamaño de los mensajes. Mensaje: <message>%1</"
"message>"

#: account.cpp:1406
msgctxt "@info error message"
msgid "Error while try to get the mail sizes. All mails are disappeard."
msgstr ""
"Error al intentar obtener el tamaño de los mensajes. Todos los mensajes han "
"desaparecido."

#: account.cpp:1422
msgctxt "@info error message"
msgid "Get corrupt size list. No spaces"
msgstr "Lista de tamaños corrupta. Sin espacios"

#: account.cpp:1434
msgctxt "@info error message"
msgid "Get corrupt size list. No numbers at begin."
msgstr "Lista de tamaños corrupta. Sin números al inicio."

#: account.cpp:1446
msgctxt "@info error message"
msgid "Get corrupt size list. No sizes found at end."
msgstr "Lista de tamaños corrupta. Sin tamaños al final."

#: account.cpp:1510
msgctxt "@info error message: could not get a mail header"
msgid ""
"<resource>%1</resource> has not sent the header of mail <numid>%2</numid>."
msgstr ""
"<resource>%1</resource> no ha enviado la cabecera del mensaje <numid>%2</"
"numid>."

#: account.cpp:1518
msgctxt "@info error message: a server doesn't know the command TOP"
msgid ""
"<resource>%1</resource> doesn't support the TOP command. KShowmail can't "
"work without this. Error message is: <message>%2</message>"
msgstr ""
"<resource>%1</resource> no admite el comando TOP. KShowmail no puede "
"funcionar sin ésto. Mensaje de error: <message>%2</message>"

#: account.cpp:1669
msgctxt "@info error message: no server responce after we have deleted a mail"
msgid ""
"<resource>%1</resource> has not sent a answer after removing of mail <numid>%"
"2</numid>."
msgstr ""
"<resource>%1</resource> no ha enviado una respuesta tras eliminar el mensaje "
"<numid>%2</numid>."

#: account.cpp:1677
msgctxt "@info error message: error while removing a mail"
msgid ""
"Error while removing mail <numid>%1</numid> of <resource>%2</resource>: "
"<message>%3</message>"
msgstr ""
"Error al eliminar el mensaje <numid>%1</numid> de <resource>%2</resource>: "
"<message>%3</message>"

#: account.cpp:1712
msgctxt "@info error message: we could not download a mail"
msgid ""
"<resource>%1</resource> has not sent an answer after retrieve of mail <numid>"
"%2</numid>."
msgstr ""
"<resource>%1</resource> no ha enviado una respuesta tras recuperar el "
"mensaje <numid>%2</numid>."

#: account.cpp:1720 account.cpp:1862
msgctxt "@info error message: we could not download a mail"
msgid ""
"Error while downloading mail <numid>%1</numid> of <resource>%2</resource>: "
"<message>%3</message>"
msgstr ""
"Error al descargar el mensaje <numid>%1</numid> de <resource>%2</resource>: "
"<message>%3</message>"

#: account.cpp:1854
msgctxt "@info error message: we could not download a mail"
msgid ""
"<resource>%1</resource> has not sent a answer after retrieve of mail <numid>%"
"2</numid>."
msgstr ""
"<resource>%1</resource> no ha enviado una respuesta tras recuperar el "
"mensaje <numid>%2</numid>."

#: account.cpp:2079
msgctxt ""
"@info error message: writing a mail into a mailbox: this is not a mail box"
msgid "<filename>%1</filename> is not a mailbox."
msgstr "<filename>%1</filename> no es un buzón de correo."

#: account.cpp:2094
msgctxt "@info error message"
msgid ""
"Can't read the hostname of your computer. But KShowmail need it to write a "
"mail into the mailbox."
msgstr ""
"No se puede leer el nombre de equipo de su sistema, pero KShowmail lo "
"necesita para guardar un mensaje en su buzón de correo."

#: account.cpp:2118
msgctxt "@info error message: we could not write a mail into a mailbox"
msgid "Could not file a mail to <filename>%1</filename>."
msgstr "No se ha podido almacenar un mensaje en <filename>%1</filename>."

#: account.cpp:2129
msgctxt "@info error message: error during writing a mail into a mailbox"
msgid ""
"Could not move a mail from <filename>%1</filename> to <filename>%2</"
"filename>."
msgstr ""
"No se ha podido mover un mensaje de <filename>%1</filename> a <filename>%2</"
"filename>."

#: account.cpp:2170
msgctxt "@info"
msgid ""
"You want to check your mails for spam, but SpamAssassin is not running.<nl/"
">KShowmail skips the spam check."
msgstr ""
"Quiere analizar sus mensajes para comprobar el spam, pero SpamAssassin no se "
"está ejecutando.<nl/>KShowmail se saltará la comprobación del spam."

#: account.cpp:2170
msgctxt "@title:window"
msgid "SpamAssassin is not running"
msgstr "SpamAssassin no se encuentra en ejecución"

#: account.cpp:2273
msgctxt "@info error message: general SSL error"
msgid "SSL error: <message>%1</message><nl/>Do you want to continue?"
msgstr "Error de SSL: <message>%1</message><nl/>¿Desea continuar?"

#: account.cpp:2274
msgctxt "@title:window"
msgid "SSL-Error - %1"
msgstr "Error-SSL - %1"

#: account.cpp:2369
msgctxt "@info error message"
msgid "Timeout"
msgstr "Tiempo límite"

#: account.cpp:2377
msgctxt "@info the task was canceled by user"
msgid "Task canceled"
msgstr "Trabajo cancelado"

#: account.cpp:2427
msgctxt "@info error message: this server doesn't support secure login"
msgid ""
"Account <resource>%1</resource>: This server doesn't provide a safety login "
"and you have disallowed the using of an unsafe login. If you want to use "
"this Account you must allow unsafe login at the account setup.<nl/"
